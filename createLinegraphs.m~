function createLinegraphs()

% Load appropriate directories
[Filename,PathName,~] = uigetfile('*.xlsx'); % prompt for Excel file
[~,~,raw] = xlsread([PathName '/' Filename]); % load Excel file

close all

raw(1,:) = [];
raw(46:end,:) = [];
raw = sortcell(raw,[3 5]);
PLMS5hr = zeros(size(raw,1), 3);
for i = 1:size(raw,1);
    switch raw{i,5}
        case 'RA'
            raw{i,5} = 1;
        case 'O2'
            raw{i,5} = 2;
        case 'NHF'
            raw{i,5} = 3;
    end
end

figure
scatter(cell2mat(raw(:,5)),cell2mat(raw(:,9)));
xlim([0.5 3.5]);

nights = 0;
prev = raw{1,3};

for i = 1:size(raw,1);
    patientID = raw{i,3};
    if patientID == prev
        nights = nights + 1;
    else
        hold on
        plot(cell2mat(raw(i-nights:i-1,5)),cell2mat(raw(i-nights:i-1,9)));
        hold on
        nights = 1;
    end
    prev = raw{i,3};
    patientNum = str2num(patientID(-3:end));
    PLM5Shr(patientNum,  = raw{i,9};
end

clearvars

end